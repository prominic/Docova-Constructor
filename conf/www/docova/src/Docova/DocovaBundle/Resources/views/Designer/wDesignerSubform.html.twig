						<td colspan="4" style="border-top:1px #CCC dashed;">
						<div class="option_container">
							<table id="combo_options" class="nofix">
								<tr>
									<td>{% trans %}Option{% endtrans %}:</td><td><input class="text ui-widget-content ui-corner-all" type="text" name="combo_label1" value="" ></td>
									<td>{% trans %}Value{% endtrans %}:</td><td><input class="text ui-widget-content ui-corner-all" type="text" name="combo_value1" value="" ></td>
									<td class="sm"><img alt="{% trans %}add{% endtrans %}" src="{{ asset('bundles/docova/images/plus.png') }}" id="add_cmboption"></td>
								</tr>
							</table>
						</div>
						</td>
					</tr>
					<tr class="variable tarea_rows" style="display:none;">
						<td>{% trans %}Columns{% endtrans %}: </td><td><input class="text ui-widget-content ui-corner-all" type="text" name="tarea_cols" value=""></td>
