						<tr>
							<td>{% trans %}On Click{% endtrans %}: <br>
								<span class="wtrmark">{% trans %}JS function to call when element is clicked.{% endtrans %}</span>
							</td><td><input type="text" name="onClickEvent" value="" ></td>
						</tr>
						<tr>
							<td>{% trans %}On Change{% endtrans %}: <br>
								<span class="wtrmark">{% trans %}JS function to call when element value changes.{% endtrans %}</span>
							</td><td><input type="text" name="onChangeEvent" value="" ></td>
						</tr>
						<tr>
							<td>{% trans %}On Focus{% endtrans %}:<br>
								<span class="wtrmark">{% trans %}JS function to call when element gets focus.{% endtrans %}</span> 
							</td><td><input type="text" name="onFocusEvent" value="" ></td>
						</tr>
						<tr>
							<td>{% trans %}On Blur{% endtrans %}: <br>
								<span class="wtrmark">{% trans %}JS function to call when element loses focus.{% endtrans %}</span>
							</td><td><input type="text" name="onBlurEvent" value="" ></td>
						</tr>
					</table>
				</div>
			</div>
		</form>
	</div>
	<div id="static_properties">
		<table>
			<tr>
				<td>ID: </td><td><input class="text ui-widget-content ui-corner-all" id="static_id" type="text" value="" ></td>
				<td>Class: </td><td><input class="text ui-widget-content ui-corner-all" id="static_class" type="text" value="" ></td>
			</tr>
			<tr>
				<td colspan="4">
					<label>Style: </label>
					<textarea id="static_style" class="text ui-widget-content ui-corner-all"></textarea>
				</td>
			</tr>
		</table>
	</div>
